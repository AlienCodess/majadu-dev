{
  "name": "alien-codes-template",
  "version": "1.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile-css": "sass css/main.scss css/main.css && postcss --use autoprefixer -r css/main.css css/main.css && css-minify -f css/main.css -o css",
    "compile-js": "rollup js/main.js --file ./js/bundle.js --format cjs && terser ./js/bundle.js -c -m -o ./js/main.min.js",
    "compile-all": "npm-run-all compile-css compile-js",
    "purge-css": "purgecss --keyframes --css css/main.min.css --content ./*.html \"node_modules/bootstrap/js/dist/{util,modal}.js\" --output css/"
  },
  "author": "Jelle Dhooghe",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "5.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "css-minify": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "purgecss": "^4.1.3",
    "rollup": "^2.74.1",
    "terser": "^5.13.1"
  }
}
